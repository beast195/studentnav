@model IEnumerable<StudentNav.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}
<div class="makedivscroll">
    <h2>Users</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProfileImagePath)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Institution)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Surname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Race)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstitutionType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HighSchool)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FieldOfStudy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Province)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Grade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LevelOfStudy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
