@model IEnumerable<StudentNav.Models.UserViewModel>

    @{
        ViewBag.Title = "SuspendUser";
    }

    <h2>SuspendUser</h2>

    <div class="row">
        <div class="col-md-12 agencies">
            @foreach (var user in Model)
            {
            <div class="row">
                <div class="col-md-2"><img src="@user.ProfilePic" style="width:100%" /></div>
                <div class="col-md-8">@user.Name</div>
                <div class="col-md-2"><a href="/Manage/SuspendUser?userId=@user.Id">Suspend</a></div>
            </div>
            <hr />
            }
</div>
</div>